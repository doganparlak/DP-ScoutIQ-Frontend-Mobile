{
  "expo": {
    "name": "ScoutWise",
    "slug": "dp-scoutiq",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "scheme": "dpscoutiq",
    "icon": "./assets/scoutwise_logo_green.png",
    "ios": {
      "icon": "./assets/scoutwise_logo_green.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.dp-scoutiq",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleDisplayName": "ScoutWise"
      }
    },
    "android": {
      "package": "com.dpformance.dpscoutiq",
      "adaptiveIcon": {
        "foregroundImage": "./assets/scoutwise_logo_green_foreground.png",
        "backgroundColor": "#1B9346"
      }
    },

    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "react-native-iap",
      [
        "react-native-google-mobile-ads",
        {
          "iosAppId": "ca-app-pub-2754612075301490~1593540774",
          "androidAppId": "ca-app-pub-3940256099942544~3347511713"
        }
      ]
    ]
    ,
    "extra": {
      "eas": {
        "projectId": "ff45af19-a14f-462f-94f2-3efd08d55f4f"
      }
    }
  }
}
